#include <iostream>
using namespace std;

int main()
{	
	string name, address, school, dept;
	int age, num;
	cout << "What is your name? \n";
	cin >> name;
	cout << "How old are you? \n";
	cin >> age;
	cout << "Where do you live? \n";
	cin >> address;
	cout << "What is your phone number? \n";
	cin >> num;
	cout << "What University are you attending? \n";
	cin >> school;
	cout << "In what department? \n";
	cin >> dept;
	
	cout << "\n\n\t Profile \n";
	cout << "\n Name: " << name;
	cout << "\n Age: " << age;
	cout << "\n Address: " << address;
	cout << "\n Phone Number: " << num;
	cout << "\n School/University: " << school;
	cout << "\n Department: " << dept;
	
	return 0;
}
